#include <stdio.h>
int main() {
    int n;
    
   
    scanf("%d", &n);
    
    int nums[n];
    
   
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    
    int result = 0;
    
   
    for (int i = 0; i < n; i++) {
        result ^= nums[i];
    }
    
   
    printf("%d\n", result);
    
    return 0;
}
